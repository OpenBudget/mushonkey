{
  "name": "mushonkey",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "copy-license": "cp LICENSE dist/mushonkey",
    "copy-readme": "cp README.md dist/mushonkey",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "build_lib": "ng build mushonkey",
    "npm_pack": "cd dist/mushonkey && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack",
    "publish": "npm run package && npm publish ./dist/mushonkey/mushonkey*.tgz"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "d3-array": "^3.2.4",
    "d3-path": "^3.1.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@types/d3-array": "^3.0.6",
    "@types/d3-path": "^3.0.0",
    "@types/d3-scale": "^4.0.4",
    "@types/d3-selection": "^3.0.6",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^16.2.0",
    "typescript": "~5.1.3"
  }
}
